load("vlasov-calc/single-vlasov-vol")$
load(stringproc)$

for c : 1 thru 1 do (
  for v : c thru 2 do (
   fname : sconcat("~/max-out/VlasovMax", c, "x", v, "v", ".cpp"),
   fh : openw(fname),
   funcName : sconcat("VlasovVol", c, "x", v, "v", "Max"),
   calcVlasovVolUpdater(fh, funcName, c, v, "Max"),
   close(fh)
  )
);
