load("vlasov-calc/vlasov-stream-surf")$
load(stringproc)$

for c : 2 thru 2 do (
  for v : c thru 3 do (
   fname : sconcat("~/max-out/VlasovSurfStreamSer", c, "x", v, "v", ".cpp"),
   fh : openw(fname),
   funcName : sconcat("VlasovSurfStream", c, "x", v, "v", "Ser"),
   calcVlasovSurfUpdater(fh, funcName, c, v, "Ser"),
   close(fh)
  )
);