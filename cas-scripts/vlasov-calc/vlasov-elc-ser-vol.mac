load("vlasov-elc-vol")$
load(stringproc)$

for c : 1 thru 2 do (
  for v : c thru 3 do (
   fname : sconcat("~/max-out/VlasovElcSer", c, "x", v, "v", ".cpp"),
   fh : openw(fname),
   funcName : sconcat("VlasovVolElc", c, "x", v, "v", "Ser"),
   calcVlasovVolUpdater(fh, funcName, c, v, "Ser"),
   close(fh)
  )
);

