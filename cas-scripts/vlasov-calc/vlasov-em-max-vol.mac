load("vlasov-calc/vlasov-em-vol")$
load(stringproc)$

for c : 3 thru 3 do (
  for v : c thru 3 do (
   fname : sconcat("~/max-out/VlasovElcMagMax", c, "x", v, "v", ".cpp"),
   fh : openw(fname),
   funcName : sconcat("VlasovVolElcMag", c, "x", v, "v", "Max"),
   calcVlasovVolUpdater(fh, funcName, c, v, "Max"),
   close(fh)
  )
);

