load("vlasov-calc/vlasov-em-glf-surf")$
load(stringproc)$

for c : 3 thru 3 do (
  for v : c thru 3 do (
   fname : sconcat("~/max-out/VlasovSurfElcMagSer", c, "x", v, "v", ".cpp"),
   fh : openw(fname),
   funcName : sconcat("VlasovSurfElcMag", c, "x", v, "v", "Ser"),
   calcVlasovSurfUpdater(fh, funcName, c, v, "Ser"),
   close(fh)
  )
);

