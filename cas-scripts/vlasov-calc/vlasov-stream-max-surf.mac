load("vlasov-calc/vlasov-stream-surf")$
load(stringproc)$

for c : 1 thru 1 do (
  for v : c thru 3 do (
   fname : sconcat("~/max-out/VlasovSurfStreamMax", c, "x", v, "v", ".cpp"),
   fh : openw(fname),
   funcName : sconcat("VlasovSurfStream", c, "x", v, "v", "Max"),
   calcVlasovSurfUpdater(fh, funcName, c, v, "Max"),
   close(fh)
  )
);