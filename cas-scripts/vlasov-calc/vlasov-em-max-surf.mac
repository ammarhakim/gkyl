load("vlasov-calc/vlasov-em-surf")$
load(stringproc)$

for c : 1 thru 2 do (
  for v : c thru 3 do (
   fname : sconcat("~/max-out/VlasovSurfElcMagMax", c, "x", v, "v", ".cpp"),
   fh : openw(fname),
   funcName : sconcat("VlasovSurfElcMag", c, "x", v, "v", "Max"),
   calcVlasovSurfUpdater(fh, funcName, c, v, "Max"),
   close(fh)
  )
);

