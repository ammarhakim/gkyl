load("pb-calc/canonical-vol")$
load(stringproc)$

pmax : 2$
for c : 1 thru 2 do (
   v : c,
   fname : sconcat("~/max-out/CanonicalMax", c, "x", v, "v", ".cpp"),
   fh : openw(fname),
   funcName : sconcat("CanonicalVol", c, "x", v, "v", "Max"),
   calcCanonicalPBVolUpdater(fh, funcName, c, v, "Max", pmax),
   close(fh)
);

