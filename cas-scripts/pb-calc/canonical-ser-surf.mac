load("pb-calc/canonical-surf")$
load(stringproc)$

pmax : 2$
for c : 1 thru 2 do (
   v : c,
   fname : sconcat("~/max-out/CanonicalSurfSer", c, "x", v, "v", ".cpp"),
   fh : openw(fname),
   funcName : sconcat("CanonicalSurf", c, "x", v, "v", "Ser"),
   calcCanonicalPBSurfUpdater(fh, funcName, c, v, "Ser", pmax),
   close(fh)
);

