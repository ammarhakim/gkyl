import os

def build(bld):
    bld.shlib(source = r"""
    gkyl_ipow.cpp
    whereami.c
    base64.cpp
    lfs.c""",
              use = "LUAJIT", includes = '.', target = 'gklib', name = 'lib', vnum = '1.0')
